<p id="notice"><%= notice %></p>

<h1>Enderecos</h1>

<table>
  <thead>
    <tr>
      <th>Cep</th>
      <th>Address</th>
      <th>Complement</th>
      <th>District</th>
      <th>City</th>
      <th>State</th>
      <th>Ibge cod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enderecos.each do |endereco| %>
      <tr>
        <td><%= endereco.cep %></td>
        <td><%= endereco.address %></td>
        <td><%= endereco.complement %></td>
        <td><%= endereco.district %></td>
        <td><%= endereco.city %></td>
        <td><%= endereco.state %></td>
        <td><%= endereco.ibge_cod %></td>
        <td><%= link_to 'Show', endereco %></td>
        <td><%= link_to 'Edit', edit_endereco_path(endereco) %></td>
        <td><%= link_to 'Destroy', endereco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Endereco', new_endereco_path %>
